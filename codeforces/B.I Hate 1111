//https://codeforces.com/contest/1526/problem/B
#include <bits/stdc++.h>
 
using namespace std;
#define ff(i , a, b) for(int i = a; i <= int(b); i++)
#define REP(i, a, b) for(int i = a; i >= int(b); i--)
#define PB push_back
#define MP make_pair
typedef pair<int , int> pi;
typedef long long ll;
const ll MAXN = 1e6 + 7;
 
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while(t--)
    {
        ll x;
        cin >> x;
        int val = x/11 , y = x%11;
        if (val/10 >= y) cout << "YES" << "\n";
        else cout << "NO" << "\n";
    }
 
    return 0;
}
