//https://codeforces.com/edu/course/2/lesson/6/2/practice/status
#include <bits/stdc++.h>
 
using namespace std;
#define ff(i , a, b) for(int i = a; i <= int(b); i++)
#define REP(i, a, b) for(int i = a; i >= int(b); i--)
#define MP make_pair
#define PB push_back
typedef long long ll;
typedef pair<int , int> pi;
const ll MAXN = 1e6 + 7;
ll num_b = 0 , num_s = 0 , num_c = 0;
ll b1 , s1 , c1; //intrinsic
ll price_b , price_s , price_c; //price of each ingredient
ll val;
bool f(ll m)
{
    ll x = m*num_b - b1 , y = m*num_s - s1 , z = m*num_c - c1;
    ll money = 0;
    if (x >= 0) money +=x*price_b;
    if (y >= 0) money +=y*price_s;
    if (z >= 0) money +=z*price_c;
    if (money <= val) return true;
    else return false;
}
int  main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);
    //freopen("X.inp" , "r" , stdin);
    //freopen("Y.out" , "w" , stdout);
    string str;
    cin >> str;
    int len = str.size();
    ff(i , 0 , len - 1)
    {
        if (str[i] == 'B') num_b++;
        else if (str[i] == 'S') num_s++;
        else num_c++;
    }
    cin >> b1 >> s1 >> c1;
    cin >> price_b >> price_s >> price_c;
    cin >> val;
    ll l = 0 , r = 1e14;
    ll ans;
    while(l <= r)
    {
        ll m = (l + r)/2;
        if (f(m)) ans = m , l = m + 1;
        else r = m - 1;
    }
    cout << ans;
    return 0;
}
